{
  "version": "0.2.0",
  "configurations": [
    {
      "runtimeArgs": ["--experimental-vm-modules"],
      "type": "node",
      "request": "launch",
      "name": "Jest All",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": ["--experimental-vm-modules node_modules/jest/bin/jest.js --coverage --runInBand"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "test"
      },
      "windows": {
        "program": "${workspaceFolder}/node_modules/jest/bin/jest"
      }
    },
    {
      "runtimeArgs": ["--experimental-vm-modules"],
      "type": "node",
      "request": "launch",
      "name": "Jest Current File",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": ["--runTestsByPath", "${relativeFile}", "--config", "jest.config.js"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "test"
      },
      "internalConsoleOptions": "neverOpen",
      "windows": {
        "program": "${workspaceFolder}/node_modules/jest/bin/jest"
      },
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Init",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:init"],
      "env": {
        "SDEBUG": "true",
        "STYPE": "init"
      },
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Update",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:update"],
      "env": {
        "SDEBUG": "true",
        "STYPE": "update"
      },
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Build",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:build"],
      "env": {
        "SDEBUG": "true",
        "STYPE": "build"
      },
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Template",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:template"],
      "env": {
        "SDEBUG": "true",
        "STYPE": "template"
      },
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Template Rebuild",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:templateRebuild"],
      "env": {
        "SDEBUG": "true",
        "STYPE": "templateRebuild"
      },
      "console": "integratedTerminal"
    }
  ]
}
